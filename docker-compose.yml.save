version: '3.3'


services:


  mongo-db:

    image: mongo

    ports:

      - '27117:27117'


  server:

    build: node-backend

    image: sanjaysaini2000/meanstack_backend
    ports:

     - '8000:8000'

    links:

      - mongo-db


  client:

    build: angular

    image: meanstack_frontend

    ports:

     - '80:4200'
